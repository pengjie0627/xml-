<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	根据框架提供的.xsd文件,写出自己的XML文件
	1.引入schema约束文档
	(bean | import)*:bean和import可以出现任意多次,并且顺序随意
	(bean|import*):要么写一个bean,要么写import任意个
	(bean*|import*):要么写任意个bean,要么写任意个import
	(bean*,import*):先写任意个bean后写任意个import
 -->
 
 <!-- beans代表根元素 -->
 <!-- xmlns="http://www.itcast.cn/bean" 这个一定要指向对应的xsd文件中的targetNamespace,否则对应的.xsd文件中的定义元素无法生效 -->
 <!-- xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 固定格式 -->
 <!-- xsi:schemaLocation="http://www.itcast.cn/bean bean-schema.xsd" 指定要使用的.xsd文件的名称-->
 
 
 
<beans xmlns="http://www.itcast.cn/bean"
	  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:schemaLocation="http://www.itcast.cn/bean bean-schema.xsd"
	>
	<bean className="" id="">
		<property value=""/>
	</bean>
	<bean className="" id="">
		<property value=""/>
	</bean>
	<import resource=""/>
</beans>